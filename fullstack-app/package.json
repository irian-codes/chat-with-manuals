{
  "name": "chat-with-manuals",
  "version": "0.1.21",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.12.0"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  },
  "scripts": {
    "build": "next build",
    "check": "npm run lint && npm run typecheck",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "prisma db seed",
    "trigger:dev": "npx trigger.dev@latest dev",
    "dev": "npx concurrently --kill-others --names \"next,trigger\" --prefix-colors \"yellow,blue\" \"next dev --turbopack\" \"npm run trigger:dev\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "pretty": "npm run format:write",
    "postinstall": "prisma generate; npx typesync",
    "lint": "SKIP_ENV_VALIDATION=true next lint",
    "lint:fix": "SKIP_ENV_VALIDATION=true next lint --fix",
    "lint-staged": "SKIP_ENV_VALIDATION=true lint-staged",
    "preview": "next build && npm run start",
    "start": "npx concurrently --kill-others --names \"next,trigger\" --prefix-colors \"yellow,blue\" \"next start\" \"npm run trigger:dev -- --env-file .env.production\"",
    "typecheck": "tsc --noEmit",
    "commit": "git add . && cz",
    "commitlint": "commitlint --edit",
    "prepare": "cd .. && husky fullstack-app/.husky",
    "release": "commit-and-tag-version"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.11.1",
    "@hookform/resolvers": "^3.10.0",
    "@langchain/community": "^0.3.29",
    "@langchain/core": "^0.3.38",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-scroll-area": "1.2.0",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.5",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tailwindcss/postcss": "^4.0.4",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@trigger.dev/sdk": "3.3.16",
    "@trpc/client": "^11.0.0-rc.741",
    "@trpc/next": "^11.0.0-rc.741",
    "@trpc/react-query": "^11.0.0-rc.741",
    "@trpc/server": "^11.0.0-rc.741",
    "chromadb": "1.9.1",
    "chromadb-default-embed": "2.13.2",
    "clamscan": "^2.4.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "entities": "^6.0.0",
    "flag-icons": "^7.3.2",
    "formidable": "^3.5.2",
    "geist": "^1.3.0",
    "gpt-tokenizer": "^2.8.1",
    "iso-639-1": "^3.1.3",
    "langchain": "^0.3.15",
    "llamaindex": "^0.9.1",
    "lru-cache": "^11.0.2",
    "lucide-react": "^0.474.0",
    "markdownlint": "^0.37.3",
    "marked": "^15.0.6",
    "marked-plaintify": "^1.1.1",
    "mime": "^4.0.6",
    "next": "15.1.6",
    "next-intl": "^3.25.1",
    "pg": "^8.13.1",
    "pgvector": "^0.2.0",
    "postcss": "^8.5.1",
    "prisma": "^6.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.0.3",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
