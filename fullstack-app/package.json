    "@types/eslint": "^9",
    "@types/eslint__eslintrc": "~2.1.2",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/formidable": "^3.4.5",
    "@types/node": "^22.12.0",
    "@types/pg": "~8.11.11",
    "@types/react": "~18.3.18",
